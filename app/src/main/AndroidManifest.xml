<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.events">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-feature android:name="android.hardware.camera2.autofocus"/>
    <uses-feature android:name="android.hardware.camera2" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ui.activity.SucessValidation"></activity>
        <activity android:name="ui.activity.ErrorValidation" />
        <activity android:name="ui.activity.LogoutPopup" />
        <activity android:name="ui.activity.Settings" />
        <activity android:name="ui.activity.DigitarQrCode" />
        <activity android:name="ui.activity.WelcomeSplash" />
        <activity android:name="ui.activity.Home" />
        <activity android:name="ui.activity.QrCodeActivity" />
        <activity android:name="ui.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ui.activity.LoginActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>